package assinment1_1;

import java.util.*;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		List<String> l=new ArrayList<>();
		Scanner sc= new Scanner(System.in);
		System.out.println("Enter 5 strings");
		for(int i=0;i<5;++i)
		{
			l.add(sc.next());
		}
	}

}
